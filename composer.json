{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "RC",
    "require": {
        "php": "^7.0.8",
        "javiereguiluz/easyadmin-bundle": "^1.17",
        "overblog/graphql-bundle": "^0.10.0@RC",
        "sensio/framework-extra-bundle": "^5.0",
        "symfony/console": "^4.0",
        "symfony/debug-bundle": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/serializer": "^4.0",
        "symfony/web-server-bundle": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01BWSR0FNGXVXP99B866TD3XF1",
            "allow-contrib": false
        }
    }
}
