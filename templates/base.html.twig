<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, minimum-scale=1.0, initial-scale=1, user-scalable=yes">

    <title>{% block title %}Apartments demo{% endblock %}</title>

    <link href="{{ asset('build/global.css') }}" rel="stylesheet">
</head>

<body>

<header class="container">
    <p>
        <a href="{{ path('homepage') }}" class="button">Home</a>
        <a href="{{ path('limited', {limit: 10}) }}" class="button">List 10</a>
        <a href="{{ path('limited', {limit: 100}) }}" class="button">List 100</a>
        <a href="{{ path('limited', {limit: 300}) }}" class="button">List 300</a>
        <a href="{{ path('filtered', {country: 'Finland'}) }}" class="button">List apartments in Finland</a>
        <a href="{{ path('filtered', {country: 'Japan'}) }}" class="button">List apartments in Japan</a>
    </p>

</header>

{% block body %}
{% endblock %}

<script type="text/javascript">
    var initialAppState = {{ appstate_serialized|raw }};
</script>

<!-- http utility -->
<script src="{{ asset('assets/js/axios.min.js') }}"></script>

<!-- typescript -->
<script src="{{ asset('build/ts-app.js') }}"></script>

<!-- vue -->
<script src="{{ asset('assets/js/vue/vue.js') }}"></script>
<script src="{{ asset('assets/js/vue/app.js') }}"></script>

