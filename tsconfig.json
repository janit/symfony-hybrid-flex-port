{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "experimentalDecorators": true,
    "noLib": false,
    "jsx": "react",
    "outDir": "dist",
    "skipLibCheck": true
  },
  "include": [
    "assets/ts"
  ],
  "exclude": [
    "node_modules"
  ]
}